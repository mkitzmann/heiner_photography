{% extends 'base.html.twig' %}

{% block content %}
    <div class="content">
        <div class="overview-container">
            {% for project in projects %}
                <a href="{{ path('GalleryRoute', {'projectTitle': project.slug}) }}">
                    <img src="{{ asset('img/thumbnails/' ~ project.thumbnail) }}" class="overviewImage">
                </a>
            {% endfor %}

        </div>
    </div>

{% endblock %}