{% extends 'base.html.twig' %}

{% block content %}
    <div class="content">
        <div class="image-container">
            {% for photo in photos %}
                <div class="slide">
                    <h2>{{ photo.getTitle }}</h2>
                    <div class="gallery-img">
                        <img  class="fullsize" src="../img/photos/{{ photo.image }}"/>
                    </div>
                </div>
            {% endfor %}
        </div>
    </div>

{% endblock %}
